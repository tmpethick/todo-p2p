{
  "name": "turtlespace",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "postinstall": "npm run build",
    "dev": "parallelshell 'npm start' 'npm run watch'",
    "start": "node server.js",
    "watch": "watchify -p livereactload -o build/bundle.js -v -d js/main.js",
    "build": "browserify js/main.js > build/bundle.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react",
            "stage-1"
          ]
        }
      ],
      "envify"

    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tmpethick/turtlespace.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tmpethick/turtlespace/issues"
  },
  "homepage": "https://github.com/tmpethick/turtlespace",
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babelify": "^7.2.0",
    "bluebird": "^3.1.1",
    "browserify": "^12.0.1",
    "express": "^4.13.3",
    "http-server": "^0.8.5",
    "peer": "^0.2.8",
    "peerjs": "^0.3.14",
    "react": "^0.14.6",
    "react-addons-css-transition-group": "^0.14.6",
    "react-dom": "^0.14.6",
    "todomvc-app-css": "^2.0.3",
    "todomvc-common": "^1.0.2",
    "uuid-js": "^0.7.5",
    "watchify": "^3.6.1"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "livereactload": "^2.1.0",
    "parallelshell": "^2.0.0",
    "react-proxy": "^1.1.2"
  }
}
